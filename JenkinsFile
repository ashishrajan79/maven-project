pipeline {
    agent any
    tools {
        maven 'localMaven'
        org.jenkinsci.plugins.docker.commons.tools.DockerTool 'localDocker'
    }
    stages{
        stage('Build'){
            steps{
                bat 'mvn clean package'
                bat "docker build . -t tomcatwebapp:${env.BUILD_ID}"
            }
        }


    }
}